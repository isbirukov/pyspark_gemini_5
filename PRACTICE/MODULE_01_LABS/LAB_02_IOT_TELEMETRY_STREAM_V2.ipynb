   ],
   "source": [
    "from pyspark.sql import SparkSession\n",
    "\n",
    "spark = SparkSession.builder \\\n",
    "        .appName(\"IOT_TELEMETRY_STREAM_V2\") \\\n",
    "        .master(\"local[*]\") \\\n",
    "        .getOrCreate()\n",
    "\n",
    "print(f\"Имя приложения: {spark.sparkContext.appName}\")\n",
    "spark.stop()"
   ]
